{
  "name": "@propsto/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm dotenv-prod -- next build",
    "build-dev": "pnpm dotenv -- next build",
    "dev": "pnpm dotenv -- pnpm next-dev",
    "dotenv-prod": "dotenv -e ../../.env.production",
    "dotenv": "dotenv -e ../../.env",
    "lint": "next lint",
    "next-dev": "next dev --hostname=$PROPSTO_WEB_HOSTNAME --port=$PROPSTO_WEB_PORT",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "framer-motion": "*"
  },
  "peerDependencies": {
    "@propsto/config": "workspace:*",
    "@propsto/constants": "workspace:*",
    "@propsto/data": "workspace:*",
    "@propsto/logger": "workspace:*",
    "@propsto/ui": "workspace:*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@vercel/og": "*",
    "autoprefixer": "*",
    "cal-sans": "*",
    "dotenv-cli": "*",
    "dotenv-expand": "*",
    "eslint": "*",
    "lucide-react": "*",
    "next": "*",
    "postcss": "*",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "*",
    "typescript": "*",
    "zod": "*"
  }
}