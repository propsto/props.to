{
  "name": "@propsto/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm dotenv -- next build",
    "dev": "pnpm dotenv -- pnpm next-dev",
    "dotenv": "dotenv -e ../../.env",
    "lint": "eslint",
    "next-dev": "next dev --hostname=$PROPSTO_WEB_HOSTNAME --port=$PROPSTO_WEB_PORT",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@propsto/config": "workspace:*",
    "@propsto/constants": "workspace:*",
    "@propsto/data": "workspace:*",
    "@propsto/logger": "workspace:*",
    "@propsto/ui": "workspace:*",
    "@vercel/analytics": "1.5.0",
    "react-hook-form": "7.60.0"
  },
  "peerDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@vercel/og": "*",
    "autoprefixer": "*",
    "cal-sans": "*",
    "dotenv-cli": "*",
    "eslint": "*",
    "framer-motion": "*",
    "lucide-react": "*",
    "next": "*",
    "postcss": "*",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "*",
    "typescript": "*",
    "zod": "*"
  }
}