{
  "name": "props.to",
  "version": "0.1.0",
  "private": true,
  "author": "Leo Giovanetti <leo@props.to>",
  "scripts": {
    "build": "turbo run build",
    "dev": "DEBUG=@propsto:* turbo run --filter !@propsto/email dev",
    "dev:app": "DEBUG=@propsto:* turbo run @propsto/app#dev @propsto/auth#dev @propsto/email#maildev @propsto/data#db-studio",
    "dev:auth": "DEBUG=@propsto:* turbo run @propsto/auth#dev @propsto/email#maildev @propsto/data#db-studio",
    "dev:email": "DEBUG=@propsto:* turbo run @propsto/email#dev",
    "dev:nuke": "turbo run @propsto/data#db-nuke",
    "dev:setup": "turbo run @propsto/data#db-setup",
    "dev:web": "DEBUG=@propsto:* turbo run @propsto/web#dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "health": "turbo run lint type-check",
    "hk": "ncu --deep && sort-package-json -i '**/node_modules' '**/package.json'",
    "postinstall": "pnpm --filter @propsto/data db-generate",
    "lint": "turbo run lint",
    "prepare": "husky",
    "setup:e2e": "pnpm exec playwright install",
    "setup:hosts": "hostile load hosts.txt",
    "test": "turbo run test",
    "test:e2e": "turbo run e2e --concurrency=1",
    "type-check": "turbo run type-check"
  },
  "dependencies": {
    "@upstash/ratelimit": "2.0.5",
    "@vercel/blob": "0.27.3",
    "@vercel/kv": "3.0.0",
    "autoprefixer": "10.4.21",
    "cal-sans": "1.0.1",
    "lucide-react": "0.485.0",
    "next": "15.2.4",
    "next-auth": "beta",
    "next-themes": "0.4.6",
    "nodemailer": "6.10.0",
    "postcss": "8.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.55.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.8.2",
    "vitest": "3.0.9",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "15.2.4",
    "@playwright/test": "1.51.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.13.14",
    "@types/nodemailer": "6.4.17",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/typescript-estree": "8.28.0",
    "dotenv-cli": "8.0.0",
    "eslint": "8.57.0",
    "framer-motion": "12.6.2",
    "hostile": "1.4.0",
    "husky": "9.1.7",
    "npm-check-updates": "17.1.16",
    "prettier": "3.5.3",
    "sort-package-json": "3.0.0",
    "start-server-and-test": "2.0.11",
    "tsx": "4.19.3",
    "turbo": "2.4.4"
  },
  "peerDependencies": {
    "@propsto/config": "workspace:*"
  },
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "pnpm": {
    "overrides": {
      "@next/eslint-plugin-next": "$@next/eslint-plugin-next",
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom",
      "@typescript-eslint/typescript-estree": "$@typescript-eslint/typescript-estree",
      "eslint": "$eslint",
      "next": "$next",
      "next-auth": "$next-auth",
      "react": "$react",
      "react-dom": "$react-dom",
      "typescript": "$typescript"
    }
  }
}