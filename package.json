{
  "name": "props.to",
  "version": "0.1.0",
  "private": true,
  "author": "Leo Giovanetti <leo@props.to>",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run --filter !@propsto/email dev",
    "debug": "turbo run --filter !@propsto/email debug",
    "dev:app": "turbo run --filter @propsto/app dev",
    "dev:auth": "turbo run --filter @propsto/auth --filter @propsto/email dev",
    "debug:auth": "turbo run --filter @propsto/auth --filter @propsto/email debug",
    "dev:web": "turbo run --filter @propsto/web dev",
    "debug:web": "turbo run --filter @propsto/web debug",
    "dev:email": "turbo run --filter @propsto/email dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint --force",
    "test": "turbo run test --force",
    "type-check": "turbo run type-check",
    "prepare": "husky"
  },
  "dependencies": {
    "autoprefixer": "10.4.20",
    "lucide-react": "0.436.0",
    "next": "15.0.0-canary.41",
    "next-themes": "0.3.0",
    "postcss": "8.4.41",
    "react": "19.0.0-rc.0",
    "react-dom": "19.0.0-rc.0",
    "tailwind-merge": "2.5.2",
    "tailwindcss": "3.4.10",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.3.3",
    "uuid": "10.0.0",
    "vitest": "2.0.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "1.46.1",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "22.5.0",
    "@types/react": "18.3.4",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "husky": "9.1.5",
    "prettier": "3.3.3",
    "tsx": "4.18.0",
    "turbo": "2.1.2"
  },
  "peerDependencies": {
    "@propsto/config": "workspace:*"
  },
  "pnpm": {
    "overrides": {
      "next": "15.0.0-canary.41",
      "react": "19.0.0-rc.0",
      "react-dom": "19.0.0-rc.0",
      "eslint": "8.57.0"
    }
  },
  "packageManager": "pnpm@9.10.0"
}